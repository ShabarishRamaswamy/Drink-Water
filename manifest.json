{
    "name": "Drink Water!",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "action": {},
    "commands": {
        "_execute_action": {
            "suggested_key": {
              "default": "Ctrl+Shift+W"
            },
            "description": "Opens hello.html"
        }
    },
    "permissions": ["storage", "scripting", "alarms", "tabs"]
}